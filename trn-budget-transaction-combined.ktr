    <connection>BIDEV - Jeanre Personal</connection>
    <sql>select * from (
select
	coalesce(trn.account, bud.account) as account
,	trn.account_number as account_number
,	coalesce(trn.account_secondary, bud.account_secondary) as account_secondary
,	coalesce(trn.budget_month_id, bud.budget_month_id) as budget_month_id
,	coalesce(trn.movement, bud.movement) as movement
,	trn.amount
,	bud.budget_amount
,	coalesce(trn.source_file_name, bud.source_file_name) as source_file_name
,	trn.balance_amount as transaction_balance_from_source
,	trn.branch
,	trn.journal_number
,	trn.narrative
,	trn.sequence
,	trn.transaction_date
,	trn.transaction_description
,	trn.transaction_type
from
	trn_transactions_split_debit_credit trn
full outer join
(
	select 
		account
	,	"Account Secondary" as account_secondary
	,	budget_month_id::date as budget_month_id
	,	movement as movement
	,	(case when amount = ' R                    -  ' then '0' else amount end)::numeric(16,4) as budget_amount
	,	source_file_name
	from 
		src_budgets
) bud on trn.budget_month_id::date = bud.budget_month_id and trn.account = bud.account and trn.account_secondary = bud.account_secondary and trn.movement = bud.movement 
) a
order by
	a.budget_month_id
,	a.account
,	a.account_secondary
,	a.movement
</sql>
