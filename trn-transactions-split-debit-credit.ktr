    <connection>BIDEV - Jeanre Personal</connection>
    <sql>select 
	account_debit
,	account_credit
,	account_number
,	amount
,	balance_amount
,	branch
,	journal_number
,	narrative
,	sequence
,	transaction_date
,	transaction_description
,	transaction_type
,	coalesce(FIRST_VALUE(first_budget_month) over (partition by grouping order by transaction_date, journal_number), '1900-01-01') as budget_month_id
,	source_file_name
from
(
	select
		*
	,	count(first_budget_month) over (order by transaction_date, journal_number) as grouping
	from
	(
		select 
			* 
		,	case when account_debit = 'Salary' then date_trunc('MONTH', transaction_date)::date + interval '1 Month' else null end as first_budget_month
		from 
			trn_transactions_double_entry 
		order by 
			transaction_date, journal_number asc
	) a
)b </sql>
